<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve" owl="2">
    <!--
        Template OWL para el botón de prueba de FE desde POS.

        Aparece como un botón en la barra de controles de la ProductScreen
        (gracias a ProductScreen.addControlButton en el JS).
    -->
    <t t-name="clocky_accounting_integration.ClockyFePosTestButton">
        <button class="button btn btn-light fw-bold" t-on-click="click">
            <i class="fa fa-paper-plane me-2"/>
            <t t-esc="_t('Probar FE GAS')"/>
        </button>
    </t>
</templates>
